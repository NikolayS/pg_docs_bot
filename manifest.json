{
  "name": "pg_docs_bot",
  "version": "0.5.3",
  "description": "Replaces links to old versions of the PostgreSQL docs with links to the current version on DuckDuckGo (for now).",
  "homepage_url": "https://github.com/mchristofides/pg_docs_bot",
  "manifest_version": 2,
  "icons": {
    "48": "slonik.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["redirectdocs.js"]
  }
}
