{
  "name": "pg_docs_bot",
  "version": "0.7.0",
  "description": "Redirects from old version of the Postgres docs to the latest (except when you're on the docs site).",
  "homepage_url": "https://github.com/mchristofides/pg_docs_bot",
  "manifest_version": 2,
  "icons": {
    "48": "slonik.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.postgresql.org/docs/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.postgresql.org/docs/*"],
      "js": ["pgdocsbotnotice.js"]
    }
  ],
  "background": {
    "scripts": ["redirectdocs.js"]
  }
}
